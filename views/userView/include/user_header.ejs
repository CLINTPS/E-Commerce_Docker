<body>
    <header class="main_nav" style="border: .5px solid rgb(0, 0, 0)">
        <div class="top_bar">
            <p style="margin: 0;">FREE SHIPPING & RETURNS ON ALL US ORDERS</p>
        </div>
        <div class="nav_bar">
            <div class="all_items">
                <div class="logo_section">
                    <a href="/user/home">

                        <img src="/images/Mobi Cart Logo.png" alt="" style="width: 120px;">
                    </a>
                </div>
                <div class="links">
                    <a href="/user/home">Home</a>
                    <a href="" data-toggle="modal" data-target="#exampleModalCenter">Contact</a>                 
                    <a href="">About</a>
                </div>
                <div class="icons">
                    <form action="/serchAllPorductsShop" method="post">
                        <div class="search_box">
                            <input type="search" name="searchAllProduct" placeholder="search product">
                            <img src="/images/search.svg" alt="">
                        </div>
                    </form>
                    <a href="/allProducts">
                        <img width="30" height="28" src="https://img.icons8.com/ios/50/shopping-mall.png" alt="shopping-mall"/>
                    </a>
                    <a href="/userWhislist">
                        <img width="30" height="28" src="https://img.icons8.com/fluency-systems-regular/48/like--v1.png" alt="like--v1"/>
                    </a>
                    <a href="/user/cart">
                        <img width="29" height="33" src="https://img.icons8.com/parakeet-line/48/shopping-cart.png" alt="shopping-cart"/>
                    </a>
                    
                    <!-- <a href="">
                        <img class="profile" src="/images/user.png" alt="">
                    </a> -->
                    <div class="dropdown">
                        <%if(user){%>
                            <button class="btn btn-outline-success dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                              <%=user%>
                            </button>
                            <%}else{%>
                                <button class="btn btn-outline-success dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    Account
                                  </button>
                                <%}%>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                          <a class="dropdown-item" href="/user/profile">Profile</a>
                          <a class="dropdown-item" href="/user/logout">Logout</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Modal -->
