<body>
    <header class="main_nav" style="border: 1px solid black">
        <div class="top_bar">
            <p style="margin: 0;">FREE SHIPPING & RETURNS ON ALL US ORDERS</p>
        </div>
        <div class="nav_bar">
            <div class="all_items">
                <div class="logo_section">
                    <a href="/user/home">

                        <img src="/images/Mobi Cart Logo.png" alt="" style="width: 120px;">
                    </a>
                </div>
                <div class="links">
                    <a href="/user/home">Home</a>
                    <a href="">Contact</a>
                    <a href="">About</a>
                </div>
                <div class="icons">
                    <form action="/serchAllPorductsShop" method="post">
                        <div class="search_box">
                            <input type="search" name="searchAllProduct" placeholder="search product">
                            <img src="/images/search.svg" alt="">
                        </div>
                    </form>
                    <a href="/allProducts">
                        <img width="30" height="28" src="https://img.icons8.com/ios/30/000000/shopping-mall.png" alt="shopping-mall"/>
                        <!-- <img width="34" height="34" src="https://img.icons8.com/material-rounded/24/shop.png" alt="shop"/> -->
                    </a>
                    <a href="/user/cart">
                        <img class="cart" src="/images/cart.svg" alt="">
                        <!-- <img width="30" height="36" src="https://img.icons8.com/pulsar-line/48/shopping-cart.png" alt="shopping-cart"/> -->
                        <!-- <img width="34" height="36" src="https://img.icons8.com/sf-black-filled/64/shopping-cart.png" alt="shopping-cart"/> -->
                        <span id="cartCnt"></span>
                    </a>
                    
                    <!-- <a href="">
                        <img class="profile" src="/images/user.png" alt="">
                    </a> -->
                    <div class="dropdown">
                        <%if(user){%>
                            <button class="btn btn-outline-dark dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                              <%=user%>
                            </button>
                            <%}else{%>
                                <button class="btn btn-outline-dark dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    Account
                                  </button>
                                <%}%>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                          <a class="dropdown-item" href="/user/profile">Profile</a>
                          <a class="dropdown-item" href="/user/logout">Logout</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
